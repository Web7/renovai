mixin SliderNavy()
	section.pt-5.pb-3.position-relative.bg-primary
		.container
			.carousel.slide#carouselNavy(data-ride="false" data-interval="false" data-wrap="false")
				ol.carousel-indicators
					li.active(data-target="#carouselNavy" data-slide-to="0")
					li(data-target="#carouselNavy" data-slide-to="1")
					li(data-target="#carouselNavy" data-slide-to="2")
					li(data-target="#carouselNavy" data-slide-to="3")
					li(data-target="#carouselNavy" data-slide-to="4")
				.carousel-inner.text-white
					.carousel-item.active
						video.video-carousel.w-100
							source(src="video/personalized-complete-the-look.mp4")
